{
  "host": "127.0.0.1",
  "port": 3000,
  "interval": 10000,
  "logPath": "webmon_logs.txt",
  "logto": "fs",
  "maxFailures": 3,
  "verbose": true,
  "waittime": 5000,
  "http200": {
    "retrytimes": 1,
    "action": "nothing"
  },
  "http403": {
    "retrytimes": 2,
    "action": "restart"
  },
  "http500": {
    "retrytimes": 3,
    "action": "restart"
  },
  "timeoutConfig": {
    "retrytimes": 2,
    "action": "restart"
  }
}
  